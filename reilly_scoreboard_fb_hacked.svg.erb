<svg width="1920" height="80">
<svg 
   xmlns:dc="http://purl.org/dc/elements/1.1/"
   xmlns:cc="http://creativecommons.org/ns#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:svg="http://www.w3.org/2000/svg"
   xmlns="http://www.w3.org/2000/svg"
   xmlns:xlink="http://www.w3.org/1999/xlink"
   xmlns:sodipodi="http://sodipodi.sourceforge.net/DTD/sodipodi-0.dtd"
   xmlns:inkscape="http://www.inkscape.org/namespaces/inkscape"

width="1440" height="80" x="0" y="0">

<?rb 
    
    message = nil
    time_to_change = ''
    home_team.flag = false
    away_team.flag = false
    show_clock = false
    message_bg = "yellow"

    # priority decode team states...
    if home_team.status != ''
        message = home_team.status
        home_team.flag = true
        if message =~ /red/i
            message_bg = "red"
        end
    elsif away_team.status != ''
        message = away_team.status
        away_team.flag = true
        if message =~ /red/i
            message_bg = "red"
        end
    elsif home_team.delayed_penalty
        message = "DELAYED PENALTY"
        home_team.flag = true
    elsif away_team.delayed_penalty
        message = "DELAYED PENALTY"
        away_team.flag = true
    elsif home_team.strength != 5 or away_team.strength != 5
        show_clock = true
        smaller = [home_team, away_team].min_by { |x| x.strength }
        if smaller == home_team
            bigger = away_team
        else
            bigger = home_team
        end

        time_to_change = format_time_without_tenths_round([
            home_team.penalties.time_to_strength_change, 
            away_team.penalties.time_to_strength_change
        ].reject { |x| x == 0 }.min)

        # simple case: penalty, no empty nets
        if not bigger.empty_net and not smaller.empty_net
            if bigger.strength > smaller.strength
                bigger.flag = true
            end
            
            if bigger.strength == 5 and smaller.strength == 4
                message = "POWER PLAY"
            else
                message = "#{bigger.strength}-ON-#{smaller.strength}"
            end
        elsif bigger.empty_net and not smaller.empty_net
            bigger.flag = true
            if bigger.strength == 5 and smaller.strength == 4
                message = "EMPTY NET + PP"
            else
                message = "EMPTY #{bigger.strength+1}-ON-#{smaller.strength}"
                if bigger.strength > smaller.strength
                    bigger.flag = true
                end
            end
        elsif smaller.empty_net and not bigger.empty_net
            message = "EMPTY #{bigger.strength}-ON-#{smaller.strength+1}"
        else
            message = "WTF EMPTY NETS"
        end
    elsif home_team.empty_net and away_team.empty_net
        message = "WTF EMPTY NETS"
    elsif home_team.empty_net and not away_team.empty_net
        message = "EMPTY NET"
        home_team.flag = true
    elsif away_team.empty_net and not home_team.empty_net
        message = "EMPTY NET"
        away_team.flag = true
    end

    if home_team.flag or away_team.flag
        ppbg = "#ffff00"
    else
        ppbg = "#dddddd"
    end

    htbg = home_team.flag ? '#ffff00' : home_team.bgcolor
    htfg = home_team.flag ? '#ffff00' : home_team.fgcolor
    htfont = (home_team.fontWidth == "1") ? "Gotham XNarrow" : "Gotham Narrow"

    atbg = away_team.flag ? '#ffff00' : away_team.bgcolor
    atfg = away_team.flag ? '#ffff00' : away_team.fgcolor
    atfont = (away_team.fontWidth == "1") ? "Gotham XNarrow" : "Gotham Narrow"

    pdfont = (clock.period.size > 2) ? "Gotham XNarrow" : "Gotham Narrow"
    pdsize = (clock.period.size > 2) ? "36" : "46"
    pdy = (clock.period.size > 2) ? "47" : "50"

?>

<defs>
  <linearGradient x1="0" x2="1" id="away">
    <stop stop-color="@{ atbg }@" offset="20%"/>
    <stop stop-color="@{ atfg }@" offset="100%"/>
  </linearGradient>
  <linearGradient x1="0" x2="1" id="home">
    <stop stop-color="@{ htbg }@" offset="20%"/>
    <stop stop-color="@{ htfg }@" offset="100%"/>
  </linearGradient>

</defs>

  <path d="M 200 10 h 172 l -25 50 h -172 z" fill="url(#away)" />
  
  <path d="M 372 10 h 80 l -25 50 h -80 z" fill="#303030" />
  <path d="M 372 10 h 80 l -25 50 h -80 z" fill="#FFCC00" opacity="@{ away_blink_opacity }@"/>
  
  <path d="M 467 10 h 172 l -25 50 h -172 z" fill="url(#home)" />
  
  <path d="M 642 10 h 80 l -25 50 h -80 z" fill="#303030" />
  <path d="M 642 10 h 80 l -25 50 h -80 z" fill="#FFCC00" opacity="@{ home_blink_opacity }@"/>
    
  <!--<image xlink:href="data:image/png;base64," width="1440" height="80" x="0" y="0" />-->

<?rb if message ?>
  <path d="M 734 10 h 461 l -25 50 h -461 z" fill="@{message_bg}@" />

    <?rb if show_clock ?>
        <!-- show penalty clock if that is what we are doing -->
  <image xlink:href="data:image/png;base64," width="496" height="70" x="704" y="5" />
        <text x="1125" y="50" name="powerPlayClock" font-family="Gotham FWN Narrow" font-size="46" fill="white" stroke="black" stroke-width="5" text-anchor="middle">@{ time_to_change }@</text>
        <text x="1125" y="50" name="powerPlayClock" font-family="Gotham FWN Narrow" font-size="46" fill="white" text-anchor="middle">@{ time_to_change }@</text>

        <text x="880" y="50" name="powerPlayText" font-family="Gotham Narrow Bold" font-size="46" fill="white" stroke="black" stroke-width="5" text-anchor="middle">@{ message }@</text>
        <text x="880" y="50" name="powerPlayText" font-family="Gotham Narrow Bold" font-size="46" fill="white" text-anchor="middle">@{ message }@</text>
    <?rb else ?>
        <!-- otherwise just show centered text -->
  <path d="M 734 10 h 461 l -25 50 h -461 z" fill="yellow" />
  <image xlink:href="data:image/png;base64," width="496" height="70" x="704" y="5" />
        <text x="952" y="50" name="teamStatusText" font-family="Gotham Narrow Bold" font-size="46" fill="white" stroke="black" stroke-width="5" text-anchor="middle">@{ message }@</text>        
        <text x="952" y="50" name="teamStatusText" font-family="Gotham Narrow Bold" font-size="46" fill="white" text-anchor="middle">@{ message }@</text>        
    <?rb end ?>
<?rb end ?>

<?rb if announce.is_up ?>
  <path d="M 734 10 h 461 l -25 50 h -461 z" fill="yellow" />
  <image xlink:href="data:image/png;base64," width="496" height="70" x="704" y="5" />
  <text x="952" y="50" name="announceText" font-family="Gotham Narrow Bold" font-size="46" fill="white" stroke="black" stroke-width="5" text-anchor="middle">@{ announce.message }@</text>
  <text x="952" y="50" name="announceText" font-family="Gotham Narrow Bold" font-size="46" fill="white" text-anchor="middle">@{ announce.message }@</text>        
<?rb end ?>

<?rb if status.is_up ?>
  <path d="M 734 10 h 461 l -25 50 h -461 z" fill="white" />
  <image xlink:href="data:image/png;base64," width="496" height="70" x="704" y="5" />
  <text x="952" y="50" name="announceText" font-family="Gotham Narrow Bold" font-size="46" fill="white" stroke="black" stroke-width="5" text-anchor="middle">@{ status.text }@</text>
  <text x="952" y="50" name="announceText" font-family="Gotham Narrow Bold" font-size="46" fill="white" text-anchor="middle">@{ status.text }@</text>        
<?rb end ?>

  <text x="401" y="50" name="AwayTeamScore" font-family="Gotham Narrow Bold" font-size="46" fill="white" stroke="black" stroke-width="5" text-anchor="middle">@{ away_team.score }@</text>
  <text x="401" y="50" name="AwayTeamScore" font-family="Gotham Narrow Bold" font-size="46" fill="white" text-anchor="middle">@{ away_team.score }@</text>
  
  <text x="275" y="50" name="AwayTeamName" font-family="@{ atfont }@ Bold" font-size="46" fill="white" stroke="black" stroke-width="5" text-anchor="middle">@{ away_team.name }@</text>
  <text x="275" y="50" name="AwayTeamName" font-family="@{ atfont }@ Bold" font-size="46" fill="white" text-anchor="middle">@{ away_team.name }@</text>
  
  <text x="668" y="50" name="HomeTeamScore" font-family="Gotham Narrow Bold" font-size="46" fill="white" stroke="black" stroke-width="5" text-anchor="middle">@{ home_team.score }@</text>
  <text x="668" y="50" name="HomeTeamScore" font-family="Gotham Narrow Bold" font-size="46" fill="white" text-anchor="middle">@{ home_team.score }@</text>
  
  <text x="542" y="50" name="HomeTeamName" font-family="@{ htfont }@ Bold" font-size="46" fill="white" stroke="black" stroke-width="5" text-anchor="middle">@{ home_team.name }@</text>
  <text x="542" y="50" name="HomeTeamName" font-family="@{ htfont }@ Bold" font-size="46" fill="white" text-anchor="middle">@{ home_team.name }@</text>
  
  <text x="1385" y="@{ pdy }@" name="periodText" font-family="@{ pdfont }@ Bold" font-size="@{ pdsize }@" fill="white" stroke="black" stroke-width="5" text-anchor="middle">@{ clock.period }@</text>
  <text x="1385" y="@{ pdy }@" name="periodText" font-family="@{ pdfont }@ Bold" font-size="@{ pdsize }@" fill="white" text-anchor="middle">@{ clock.period }@</text>
  
  <text x="1275" y="50" name="clockText" font-family="Gotham FWN Narrow" font-size="46" fill="white" stroke="black" stroke-width="5" text-anchor="middle">@{ clock.time }@</text>
  <text x="1275" y="50" name="clockText" font-family="Gotham FWN Narrow" font-size="46" fill="white" text-anchor="middle">@{ clock.time }@</text>
  
</svg>
</svg>
