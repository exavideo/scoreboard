<% 
    if home_team.strength < 5 or away_team.strength < 5           
        smaller, bigger = [home_team.strength, away_team.strength].minmax

        time_to_change = format_time_without_tenths([
            home_team.penalties.time_to_strength_change, 
            away_team.penalties.time_to_strength_change
        ].reject { |x| x == 0 }.min)

        if bigger == 5 and smaller == 4
            message = "POWER PLAY #{time_to_change}"
        else
            message = "#{bigger}-ON-#{smaller} #{time_to_change}"
        end
